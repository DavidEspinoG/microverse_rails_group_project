<h2>Edit food<h2> 

<%= form_with url: food_path(@food), method: :patch do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, value: @food.name %>
  <%= form.label :quantity %>
  <%= form.text_field :quantity, value: @food.quantity %>
  <%= form.label :price %>
  <%= form.number_field :price, value: @food.price %>
  <%= form.submit %>
<% end %>